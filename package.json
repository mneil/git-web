{
  "name": "terminal-web",
  "scripts": {
    "build": "webpack",
    "test": "mocha"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^6.7.1",
    "debug": "^4.3.4",
    "deep-equal-in-any-order": "^1.1.17",
    "eventemitter2": "^6.4.5",
    "exports-loader": "^3.1.0",
    "express": "^4.18.1",
