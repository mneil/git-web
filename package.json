    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "puppeteer": "^13.7.0",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.1",
    "wasm-git": "^0.0.8",
    "webpack": "^5.70.0",
